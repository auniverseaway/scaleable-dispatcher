/url "/bin/permissioncheck.html"
/filter
    {
        /0010
            {
                /glob "*"
                /type "deny"
            }
        /0020
        {
            /glob "*.html"
            /type "allow"
        }
    }
/headers
    {
        /0010
            {
                /glob "*"
                /type "deny"
            }
        /0020
            {
                /glob "Set-Cookie:*"
                /type "allow"
            }
    }